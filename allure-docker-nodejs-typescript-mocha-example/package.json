    "chai": "4.2.0",
    "ts-node": "8.10.1",
    "typescript": "3.9.7",
    "@types/mocha": "7.0.2",
    "@types/node": "14.0.5",
    "@types/module-alias": "2.0.0"
  },
  "_moduleAliases": {
    "@utils": "dist/utils"
  }
}
